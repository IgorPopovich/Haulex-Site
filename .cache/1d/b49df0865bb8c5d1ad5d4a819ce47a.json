Wrap.css('left', '-100%');\r\n        $('.slide').last().after($('.slide').first());\r\n      });\r\n    });\r\n  })\r\n\r\n\r\n  /* --- gallery tablet--- */\r\n  $(document).ready(function() {\r\n    var mySwiper = new Swiper(\".mySwiper\", {\r\n      autoplay: {\r\n        delay: 2000,\r\n      },\r\n      speed: 800,\r\n      loop: true,\r\n      spaceBetween: 1,\r\n      slidesPerView: 3,\r\n      centeredSlides: true,\r\n      roundLengths: true,\r\n      loop: true,\r\n      loopAdditionalSlides: 30,\r\n      navigation: {\r\n        nextEl: \".swiper-button-next\",\r\n        prevEl: \".swiper-button-prev\"\r\n      }\r\n    });\r\n  });\r\n\r\n\r\n\r\n/* --- bg scale--- */\r\n  \r\n  $(window).scroll(function() {\r\n    var scroll = $(window).scrollTop();\r\n    $('.part-01').css({\r\n      width: (100 + scroll/25) + '%'\r\n    })\r\n  });"},"lineCount":null}},"error":null,"hash":"f8beed77c6108098d8df6cfeab92684d","cacheData":{"env":{}}}